{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ResourceDir": "${TINY_CMS_DIR}",
  "Kestrel": {
    "Endpoints": {
      "default": { "Url": "${TINY_CMS_URL}" }
    }
  },
  "AllowWrite": true,
  "AllowOverWrite": true,
  "AllowDelete": true,
  "AllowDeleteDir": true,
  "AllowedHosts": "*"
}
